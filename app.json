{
  "expo": {
    "name": "FitXWidget",
    "slug": "FitXWidget",
    "scheme": "fitxwidget",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.foffa.FitXWidget"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
        {
          "fonts": ["./assets/fonts/DMSans-Bold.ttf", "./assets/fonts/DMSans-Medium.ttf", "./assets/fonts/DMSans-Regular.ttf"]
        },
        "@sentry/react-native/expo",
        {
          "organization": "foffa",
          "project": "fitx-widget"
        }
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "43712921-1093-41e6-92cb-aa8894d70619"
      }
    }    
  }
}
